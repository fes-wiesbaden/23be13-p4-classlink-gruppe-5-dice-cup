{
  "name": "classlink-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "format:write": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check && npm run build -- --configuration development",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.6",
    "@angular/cdk": "^20.2.9",
    "@angular/common": "20.3.6",
    "@angular/compiler": "20.3.6",
    "@angular/core": "20.3.6",
    "@angular/forms": "20.3.6",
    "@angular/material": "^20.2.9",
    "@angular/platform-browser": "20.3.6",
    "@angular/router": "20.3.6",
    "@primeuix/themes": "^1.2.3",
    "@taiga-ui/cdk": "^4.51.0",
    "@taiga-ui/core": "^4.51.0",
    "@taiga-ui/kit": "^4.51.0",
    "@taiga-ui/layout": "^4.51.0",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "check": "^1.0.0",
    "material-icons": "^1.13.14",
    "npm-check": "^6.0.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typecheck": "^0.1.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "20.3.6",
    "@angular/cli": "20.3.6",
    "@angular/compiler-cli": "20.3.6",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0"
  }
}
