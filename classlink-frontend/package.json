{
  "name": "classlink-frontend",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "format:write": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check && npm run build -- --configuration development",
    "lint": "ng lint",
    "api:gen": "openapi-generator-cli generate -g typescript-angular -o src/app/api -i http://localhost:4000/v3/api-docs --additional-properties=fileNaming=kebab-case,ngVersion=20,providedInRoot=true,stringEnums=true,useRxJS6=true",
    "storybook": "ng run classlink-frontend:storybook",
    "build-storybook": "ng run classlink-frontend:build-storybook"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.4",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@primeuix/themes": "^1.2.3",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "check": "^1.0.0",
    "global": "^4.4.0",
    "npm-check": "^3.2.10",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typecheck": "^0.1.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.0",
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.2.0",
    "@compodoc/compodoc": "^1.1.30",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/angular": "^9.1.5",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-storybook": "^9.1.5",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.2.0",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "storybook": "^9.1.5",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0"
  }
}
